package com.company;
import java.util.*;
class Main{
    public static void main(String[]args){
        System.out.println("Enter the natural numbers: ");
        Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        //using while loop
        while(n>=1){
            System.out.print(n+"  ");
            n--;
        }
    }
}
