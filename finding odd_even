package com.company;
import java.util.*;
 public class Main{
     public static void main(String[] args){
         int num;
         System.out.println("Enter an Integer number: ");
         Scanner sc= new Scanner(System.in);
         num= sc.nextInt();
         if (num%2==0)
             System.out.println("even");
         else
             System.out.println("odd");
     }

 }
 

